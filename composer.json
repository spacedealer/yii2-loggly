		"log",
		"loggly"
	],
	"homepage": "https://github.com/spacedealer/yii2-loggly",
	"type": "yii2-extension",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Dirk Adler",
			"email": "adler@spacedealer.de"
		}
	],
	"support": {
		"issues": "https://github.com/spacedealer/yii2-loggly/issues",
		"source": "https://github.com/spacedealer/yii2-loggly"
